{
  "framework": "vite",
  "installCommand": "npm ci --include=dev",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://calorie-backend-q4r0.onrender.com/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
  